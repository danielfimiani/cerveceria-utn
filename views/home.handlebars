<section class="probootstrap-section">
    <div class="container">
        <div class="row">
            <div class="probootstrap-cell-retro">
                {{#each Especialidades}}
                {{#ifCond @index '==' '0' }}
                <div class="half">
                    {{/ifCond}}
                    {{#ifCond @index '==' '3' }}
                    <div class="half">
                        {{/ifCond}}
                        <div class="probootstrap-cell probootstrap-animate" data-animate-effect="fadeIn">
                            <div class="image" style="background-image: url({{image}});"></div>
                            <div class="text text-center">
                                <h3>{{txt_nombre}}</h3>
                                <p>{{txt_desc}}</p>
                                <p class="price">${{imp_precio}}</p>
                            </div>
                        </div>
                        {{#ifCond @index '==' '2'}}
                    </div>
                    {{/ifCond}}
                    {{#ifCond @index '==' '5'}}
                </div>
                {{/ifCond}}
                {{/each}}
            </div>
        </div>
    </div>
</section>

